    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next out",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "tensorflow",
    "handwriting-recognition",
    "math-education",
