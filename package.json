  "description": "AI-powered handwriting recognition system for elementary math learning with TensorFlow.js",
  "private": true,
  "homepage": "https://github.com/[username]/elementary-math-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/[username]/elementary-math-app.git"
  },
  "bugs": {
    "url": "https://github.com/[username]/elementary-math-app/issues"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
