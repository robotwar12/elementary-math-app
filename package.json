  "author": {
    "name": "김정민",
    "email": "[your-email]"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "framer-motion": "^12.23.0",
    "next": "^15.3.5",
    "next-pwa": "^5.6.0",
    "onnxruntime-web": "^1.22.0",
    "perfect-freehand": "^1.2.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-config-next": "^15.3.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2"
